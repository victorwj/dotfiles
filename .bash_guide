# Tarfiles
tar -zxvf extract_file.tar.gz
tar -czvf compress_file.tar.gz /path/to/file

# Enter/exit root mode
sudo -s
exit

# Manage processes
ps aux 
kill -9 PID
pkill PID

# System cleanup
sudo apt-get autoremove # Remove unneeded packages
sudo apt-get clean # Clean package cache

# Permissions
# - rwx r-- r-x
# type owner group everyone
chmod +rwx file/dir
chmod -rwx file/dir

# Generate SSH
ssh-keygen -t rsa -b 4096 -C "your_email@example.com"

# Folder size
du -sh dir
