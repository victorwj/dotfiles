# General
alias b="cd - > /dev/null"
alias cdd="cd .."
alias ll="ls -lhA"
alias l="ls -lhA"
alias miniprompt='export PS1="\[\e[90m\]\u@\h\[\e[0m\] \[\e[91m\]\`nonzero_return\`\[\e[0m\]$ "'
alias g++='g++ -x c++ -Wall -Werror -Wextra -pedantic'

# Command-specific
alias ffile="find . -name"
alias fhist="history | grep"
